{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "tiendas/**/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/tiendas/tienda1/public/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/products",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/whatsapp",
      "dest": "/api/whatsapp.js"
    },
    {
      "src": "/api/settings",
      "dest": "/api/settings.js"
    },
    {
      "src": "/(.*)",
      "dest": "/tiendas/tienda1/public/index.html"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key"
  }
}
