{ 
    "version": 2, 
    "routes": [ 
        { 
            "src": "/assets/(.*)", 
            "dest": "/public/assets/$1", 
            "headers": { 
                "Cache-Control": "public, max-age=31536000, immutable" 
            } 
        }, 
        { 
            "src": "/api/products", 
            "dest": "/api/products.js" 
        }, 
        { 
            "src": "/api/settings", 
            "dest": "/api/settings.js" 
        }, 
        { 
            "src": "/api/whatsapp", 
            "dest": "/api/whatsapp.js" 
        }, 
        { 
            "src": "/", 
            "dest": "/tiendas/tienda1/public/index.html", 
            "headers": { 
                "Cache-Control": "public, max-age=3600" 
            } 
        } 
    ] 
} 
